{
  "name": "wallmansclock-v2",
  "version": "0.0.0",
  "private": true,
  "bin":"./bin/www",
  "scripts": {
    "start": "npx nodemon ./bin/www",
    "build": "nexe ./bin/www -r public/**/* -r lib/**/* -r bin/**/* -r routes/**/* -r views/**/* -r app.js -r node_modules/**/*"
  },
  "pkg": {
    "scripts": "build/**/*.js",
    "assets": ["bin/**/*", "lib/**/*", "public/**/*", "routes/**/*", "views/**/*"]
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "csv-array": "0.0.22",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "ip": "^1.1.5",
    "morgan": "~1.9.1",
    "nexe": "^3.3.2",
    "pug": "^2.0.4",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.2"
  }
}
